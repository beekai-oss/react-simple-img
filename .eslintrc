{
  "extends": ["airbnb", "plugin:flowtype/recommended"],
  "rules": {
    "flowtype/define-flow-type": 2,
    "react/prop-types": 0,
    "react/no-unused-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "global-require": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "import/first": 0
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": ["flowtype"]
}
